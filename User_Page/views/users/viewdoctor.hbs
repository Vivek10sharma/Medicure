<link href="https://fonts.googleapis.com/css2?family=
Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;
1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css">
<link rel="stylesheet" href="/static/viewdoctor.css">

<section class="main">
        <h1>{{specialization}} Doctor</h1>
        <div class="container">
            {{#each doctor}}
            <a href="/doctordetail/{{this.doctor_id}}" style="text-decoration: none;">
                <div class="card">
                    <i class="fa-solid fa-user-doctor fa-9x"></i>
                    <div class="doctorName">Dr. {{this.doctor_name}}</div>
                    <div class="location">{{this.address}}</div>
                </div>
            </a>
            {{/each}}
        </div>
    </section>

                                   